{
  "name": "@the_ro_show/agent-ads-sdk",
  "version": "0.15.1",
  "description": "TypeScript SDK for integrating agent-native sponsored units (Sponsored Suggestions and Sponsored Tools) into AI agents",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "SECURITY.md",
    "SIMPLE_INTEGRATION_GUIDE.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rtrivedi/agent-ads-sdk.git"
  },
  "homepage": "https://github.com/rtrivedi/agent-ads-sdk#readme",
  "bugs": {
    "url": "https://github.com/rtrivedi/agent-ads-sdk/issues"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "attentionmarket",
    "agent",
    "ads",
    "ai",
    "sponsored"
  ],
  "author": "AttentionMarket",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  }
}
